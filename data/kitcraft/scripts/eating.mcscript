asat(@a[scores={kc_eattime=1..}]) {
  if(!'entity @s[nbt={SelectedItem:{tag:{kitcraft:{Eatable:1}}}}]') {
    /scoreboard players reset @s kc_eattime
  }
  if(!'entity @s[scores={kc_sneak=1..}]') {
    /scoreboard players reset @s kc_eattime
  }
}

